		<module>springboot-cache-jetcache</module>
		<module>springboot-cache-j2cache</module>
		<module>springboot-webmagic</module>
		<module>springboot-dubbo-nacos</module>
        <module>hystrix-demo</module>
        <module>springboot-shiro</module>
