        <module>springboot-xxl-job</module>
        <module>spark245-demo</module>
        <module>springboot-security2</module>
        <module>springboot-setup</module>
        <module>dubbo-sofarpc-demo</module>
        <module>jim-demo</module>
        <module>captcha-demo</module>
        <module>activiti-demo</module>
        <module>redis-caffeine-L2-cache</module>
		<module>drools-demo</module>
        <module>springboot-template</module>
    </modules>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<java.version>1.8</java.version>
		<spring-cloud.version>Finchley.SR1</spring-cloud.version>
